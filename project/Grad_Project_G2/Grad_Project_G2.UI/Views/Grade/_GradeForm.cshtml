@model GradeViewModel

<div class="mb-3">
    <label asp-for="Value" class="form-label"></label>
    <input asp-for="Value" class="form-control" />
    <span asp-validation-for="Value" class="text-danger"></span>
</div>

<div class="mb-3">
    <label asp-for="SessionId" class="form-label"></label>
    <select asp-for="SessionId" asp-items="Model.Sessions" class="form-select"></select>
    <span asp-validation-for="SessionId" class="text-danger"></span>
</div>

<div class="mb-3">
    <label asp-for="TraineeId" class="form-label"></label>
    <select asp-for="TraineeId" asp-items="Model.Trainees" class="form-select"></select>
    <span asp-validation-for="TraineeId" class="text-danger"></span>
</div>
